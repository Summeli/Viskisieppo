{
	"name": "Viskisieppo",
	"version": "1.0.2",
	"manifest_version": 2,
	"description": "Poistaa viskisivujen estot suomalaisilta",
	"homepage_url": "http://summeli.fi/",
	"options_page": "options.html",
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"*://*.glenmorangie.com/*"
	],
	"background": {
		"scripts": ["background.js"],
		"persistent": true
	},
	"icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" }
}